<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms" 
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  x:Class="i4prj.SmartCab.Views.CustomerMasterDetailPage"
                  xmlns:prism="clr-namespace:Prism.Navigation.Xaml;assembly=Prism.Forms">
    <MasterDetailPage.Master>
        <ContentPage Title="TEST">
            <StackLayout Margin="10">
                <Label Text="{Binding Customer.Name}" FontSize="Large"></Label>
                <Label Text="{Binding Customer.Email}" Margin="0,0,0,10"></Label>
                <Button Text="Log ud" Command="{Binding LogOutCommand}" />
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
    
</MasterDetailPage>
